{
  "type": "project",
  "license": "GPL-3.0",
  "name": "sedici/celsius3",
  "description": "The Celsius3 Project",
  "require": {
    "php": ">=7.4",
    "ext-ctype": "*",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-zip": "*",
    "beberlei/doctrineextensions": "^1.3",
    "composer/package-versions-deprecated": "1.11.99.2",
    "doctrine/annotations": "^1.0",
    "doctrine/data-fixtures": "~1.1",
    "doctrine/doctrine-bundle": "^2.4",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "doctrine/orm": "^2.9",
    "egulias/email-validator": "^3.1",
    "enqueue/elastica-bundle": "^0.9.3",
    "enqueue/fs": "^0.9.12",
    "excelwebzone/recaptcha-bundle": "^1.4",
    "friendsofsymfony/elastica-bundle": "^5.2",
    "friendsofsymfony/http-cache-bundle": "~2.1",
    "friendsofsymfony/jsrouting-bundle": "~2.7",
    "friendsofsymfony/message-bundle": "~2.0",
    "friendsofsymfony/oauth-server-bundle": "~1.5",
    "friendsofsymfony/rest-bundle": "~3.0",
    "friendsofsymfony/user-bundle": "dev-master",
    "incenteev/composer-parameter-handler": "~2.0",
    "jms/aop-bundle": "^1.2",
    "jms/i18n-routing-bundle": "^3.1",
    "jms/serializer-bundle": "~3.9",
    "jms/translation-bundle": "^1.3",
    "knplabs/knp-components": "^3.0",
    "knplabs/knp-menu-bundle": "~2.1",
    "knplabs/knp-paginator-bundle": "^5.6",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/httplug-bundle": "^1.7",
    "phpdocumentor/reflection-docblock": "^5.2",
    "predis/predis": "^1.0",
    "sabberworm/php-css-parser": "~8.0",
    "salavert/time-ago-in-words": "1.*",
    "sensio/framework-extra-bundle": "^5.1",
    "snc/redis-bundle": "^3.4",
    "stof/doctrine-extensions-bundle": "~1.2",
    "symfony/asset": "4.4.*",
    "symfony/console": "4.4.*",
    "symfony/dotenv": "4.4.*",
    "symfony/expression-language": "4.4.*",
    "symfony/flex": "^1.3.1",
    "symfony/form": "4.4.*",
    "symfony/framework-bundle": "4.4.*",
    "symfony/http-client": "4.4.*",
    "symfony/intl": "4.4.*",
    "symfony/mailer": "4.4.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/process": "4.4.*",
    "symfony/property-access": "4.4.*",
    "symfony/property-info": "4.4.*",
    "symfony/proxy-manager-bridge": "4.4.*",
    "symfony/security-bundle": "4.4.*",
    "symfony/serializer": "4.4.*",
    "symfony/swiftmailer-bundle": "^3.5",
    "symfony/translation": "4.4.*",
    "symfony/twig-bundle": "^4.4",
    "symfony/validator": "4.4.*",
    "symfony/web-link": "4.4.*",
    "symfony/webpack-encore-bundle": "^1.12",
    "symfony/yaml": "4.4.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "umpirsky/composer-permissions-handler": "~1.4",
    "willdurand/geocoder": "~2.8",
    "willdurand/js-translation-bundle": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.5",
    "symfony/phpunit-bridge": "~4.4",
    "symfony/stopwatch": "^4.4",
    "symfony/web-profiler-bundle": "^4.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Celsius3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Celsius3\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "writable-dirs": [
      "app/cache",
      "app/logs",
      "app/spool",
      "web/uploads"
    ],
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    }
  }
}
