{
  "type": "project",
  "license": "GPL-3.0",
  "name": "sedici/celsius3",
  "description": "The Celsius3 Project",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "ext-ctype": "*",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "beberlei/doctrineextensions": "~1.0",
    "cboden/ratchet": "0.4.*",
    "doctrine/annotations": "^1.0",
    "doctrine/data-fixtures": "~1.1",
    "doctrine/dbal": "^3.3",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.11",
    "egulias/email-validator": "^2.1",
    "enqueue/elastica-bundle": "^0.11.0",
    "enqueue/fs": "^0.10",
    "excelwebzone/recaptcha-bundle": "^1.4",
    "friendsofsymfony/elastica-bundle": "^6.0",
    "friendsofsymfony/http-cache-bundle": "~2.1",
    "friendsofsymfony/jsrouting-bundle": "^2.8",
    "friendsofsymfony/oauth-server-bundle": "dev-master",
    "friendsofsymfony/rest-bundle": "^3.3",
    "knplabs/knp-components": "^3.5",
    "knplabs/knp-menu-bundle": "^3.2",
    "knplabs/knp-paginator-bundle": "^5.8",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/httplug-bundle": "1.25.0",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.2",
    "predis/predis": "^1.0",
    "sabberworm/php-css-parser": "~8.0",
    "sensio/framework-extra-bundle": "^6.1",
    "snc/redis-bundle": "^4.1",
    "stof/doctrine-extensions-bundle": "~1.2",
    "symfony/asset": "5.4.*",
    "symfony/console": "5.4.*",
    "symfony/doctrine-messenger": "5.4.*",
    "symfony/dotenv": "5.4.*",
    "symfony/expression-language": "5.4.*",
    "symfony/flex": "^1.17|^2",
    "symfony/form": "5.4.*",
    "symfony/framework-bundle": "5.4.*",
    "symfony/http-client": "5.4.*",
    "symfony/intl": "5.4.*",
    "symfony/mailer": "5.4.*",
    "symfony/mime": "5.4.*",
    "symfony/monolog-bundle": "^3.0",
    "symfony/notifier": "5.4.*",
    "symfony/password-hasher": "5.4.*",
    "symfony/process": "5.4.*",
    "symfony/property-access": "5.4.*",
    "symfony/property-info": "5.4.*",
    "symfony/proxy-manager-bridge": "5.4.*",
    "symfony/rate-limiter": "5.4.*",
    "symfony/runtime": "5.4.*",
    "symfony/security-bundle": "5.4.*",
    "symfony/security-csrf": "5.4.*",
    "symfony/serializer": "5.4.*",
    "symfony/string": "5.4.*",
    "symfony/translation": "5.4.*",
    "symfony/twig-bundle": "5.4.*",
    "symfony/validator": "5.4.*",
    "symfony/web-link": "5.4.*",
    "symfony/webapp-meta": "^1.0",

    "symfony/yaml": "5.4.*",
    "symfonycasts/reset-password-bundle": "^1.13",
    "symfonycasts/verify-email-bundle": "^1.8",
    "twig/extensions": "^1.5",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "umpirsky/composer-permissions-handler": "~1.4",
    "willdurand/geocoder": "^4.5",
    "willdurand/js-translation-bundle": "^5.0"
  },
  "require-dev": {
    "symfony/debug-bundle": "5.4.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/stopwatch": "5.4.*",
    "symfony/web-profiler-bundle": "5.4.*",
    "fakerphp/faker": "^1.19",
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "^5.4",
    "symfony/css-selector": "^5.4",
    "symfony/phpunit-bridge": "^5.4"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Celsius3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Celsius3\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "writable-dirs": [
      "app/cache",
      "app/logs",
      "app/spool",
      "web/uploads"
    ],
    "branch-alias": {
      "dev-master": "develop"
    },
    "symfony": {
      "allow-contrib": false,
      "require": "5.4.*"
    }
  }
}
