<dl class="dl-horizontal">
    <dt>{{ 'Email'|trans }}</dt>
    <dd>{{ element.email }}</dd>
    <dt>{{ 'Address'|trans }}</dt>
    <dd>{{ element.address }}</dd>
    <dt>{{ 'Type'|trans }}</dt>
    <dd>{{ element.type.name }}</dd>
    <dt>{{ 'User'|trans }}</dt>
    <dd>{{ element.user }}</dd>
    <dt>{{ 'Instance'|trans }}</dt>
    <dd>{{ element.instance?element.instance:'-' }}</dd>
    <dt>{{ 'Owning instance'|trans }}</dt>
    <dd>{{ element.owningInstance?element.owningInstance:'-' }}</dd>
</dl>