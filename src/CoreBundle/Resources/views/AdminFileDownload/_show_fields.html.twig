<dl class="dl-horizontal">
    <dt>{{ 'IP'|trans }}</dt>
    <dd>{{ element.ip }}</dd>
    <dt>{{ 'User Agent'|trans }}</dt>
    <dd>{{ element.userAgent ? element.userAgent : '-' }}</dd>
    <dt>{{ 'Username'|trans }}</dt>
    <dd>{{ element.user.username }}</dd>
    <dt>{{ 'Date'|trans }}</dt>
    <dd>{{ element.createdAt|localizeddate }}</dd>
</dl>
