<dl class="dl-horizontal">
    <dt>{{ 'Name'|trans }}</dt>
    <dd>{{ element.name }}</dd>
    <dt>{{ 'Private?'|trans }}</dt>
    <dd>{{ element.private?'Yes'|trans:'No'|trans }}</dd>
    <dt>{{ 'Required?'|trans }}</dt>
    <dd>{{ element.required?'Yes'|trans:'No'|trans }}</dd>
    <dt>{{ 'Value'|trans }}</dt>
	<dd>{{ element.value }} </dd>
</dl>
