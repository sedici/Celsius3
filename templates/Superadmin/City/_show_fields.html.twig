<dl class="dl-horizontal">
    <dt>{{ 'Name'|trans }}</dt>
    <dd>{{ element.name }}</dd>
    <dt>{{ 'Postal code'|trans }}</dt>
    <dd>{{ element.postalCode }}</dd>
    <dt>{{ 'Country'|trans }}</dt>
    <dd>{{ element.country }}</dd>
    <dt>{{ 'Instance'|trans }}</dt>
    <dd>{{ element.instance?element.instance.name:'-' }}</dd>
</dl>
