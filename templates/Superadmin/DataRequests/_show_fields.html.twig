<dl class="dl-horizontal">
    <dt>{{ 'Initial date' | trans }}</dt>
    <dd>{{ element.startDate | localizeddate('medium', 'none') }}</dd>
    <dt>{{ 'Final date' | trans }}</dt>
    <dd>{{ element.endDate | localizeddate('medium', 'none') }}</dd>
    <dt>{{ 'Data' | trans }}</dt>
    <dd>
        {% for k,v in element.getArrayData() %}
            {% if v is iterable %}
                {% for i,a in v %}
                    {% set first = true %}
                    {{ i | trans }}{{ ' [' }}{% for e in a %}{% if not first %}{{ ', ' }}{{ e | trans | first_upper }}{% else %}{{ e | trans | first_upper }}{% set first = false %}{% endif %}{% endfor %}{{ ']' }}
                {% endfor %}
            {% else %}
                {{ v | trans | first_upper }}
            {% endif %}
            {{ ', ' }}&nbsp;
        {% endfor %}
    </dd>
    <dt>{{ 'File' | trans }}</dt>
    <dd>{{ element.file }}</dd>
</dl>